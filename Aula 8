let turno = prompt("Qual turno?")
let Matutino = "M"
let Vespertino = "V"
let Noturno = "N"

if(turno == "M"){
   console.log("valor digitado é Matutino")
}else if(turno == "V"){
    console.log("valor digitado é Vespertino")
}else {
    console.log("valor digitado é Noturno")
}
